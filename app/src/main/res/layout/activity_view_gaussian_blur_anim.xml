<?xml version="1.0" encoding="utf-8"?>
<com.jadynai.kotlindiary.view.TestViewGroupMeasureFlow
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:self="http://schemas.android.com/tools"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.jadyn.ai.acrobat.recyclerview.ArcView
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:src="@drawable/cat"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/text_res_drawable_tv"
        android:layout_width="300dp"
        android:layout_height="100dp"
        android:visibility="gone"
        android:layout_marginTop="60dp"
        android:background="@android:color/black"
        android:gravity="center"
        android:text="asdadasdasdaasdasdasdasdasdasd"
        android:textColor="@android:color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.jadynai.kotlindiary.view.ViewText
        android:id="@+id/ddddd"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.jadynai.kotlindiary.view.VideoFrameLoadingView
        android:id="@+id/view_wwwww"
        android:layout_width="300dp"
        android:layout_height="200dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </com.jadynai.kotlindiary.view.VideoFrameLoadingView>

    <com.jadynai.kotlindiary.view.TwoDirectionProgress
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginBottom="60dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</com.jadynai.kotlindiary.view.TestViewGroupMeasureFlow>